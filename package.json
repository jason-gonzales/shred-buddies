{
  "name": "shred-buddies",
  "version": "1.0.0",
  "description": "a full stack application  where user can create an event and find a shred buddy to go snowboarding",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "npm-run-all --silent --parallel dev:*",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon server/index.js",
    "db:import": "source .env && psql $DATABASE_URL < database/dump.sql",
    "db:export": "source .env && pg_dump -c --if-exists -O $DATABASE_URL > database/dump.sql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markgallardo/shred-buddies.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/markgallardo/shred-buddies/issues"
  },
  "homepage": "https://github.com/markgallardo/shred-buddies#readme",
  "dependencies": {
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "pg": "8.5.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "session-file-store": "1.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-transform-react-jsx": "7.12.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.1",
    "eslint": "7.13.0",
    "eslint-config-standard": "16.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "nodemon": "2.0.6",
    "npm-run-all": "4.1.5",
    "webpack": "5.4.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0"
  }
}
